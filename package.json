{
  "name": "command-line-toolbox",
  "type": "module",
  "version": "0.0.14",
  "packageManager": "pnpm@9.0.2",
  "description": "命令行工具箱",
  "author": "OSpoon <zxin088@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ospoon/command-line-toolbox#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ospoon/command-line-toolbox.git"
  },
  "bugs": "https://github.com/ospoon/command-line-toolbox/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "clt": "./bin/cli.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0",
    "libvips": ">=8.15.2"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@octokit/core": "^6.1.2",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "iconv-lite": "^0.6.3",
    "inquirer": "^9.2.19",
    "minimatch": "^9.0.4",
    "minimist": "^1.2.8",
    "ora": "^8.0.1",
    "picocolors": "^1.0.0",
    "shelljs": "^0.8.5",
    "ts-morph": "^22.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/inquirer": "^9.0.7",
    "@types/minimatch": "^5.1.2",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.12.7",
    "@types/shelljs": "^0.8.15",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.0.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
